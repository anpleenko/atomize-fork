{
  "name": "atomize-fork",
  "displayName": "Atomize Fork (Atom One Dark theme)",
  "description": "A detailed and accurate Atom One Dark Theme",
  "version": "2.2.3",
  "publisher": "anpleenko",
  "icon": "images/icon.png",
  "author": {
    "name": "anpleenko"
  },
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "color",
    "theme",
    "atom",
    "dark",
    "one dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Atomize fork",
        "uiTheme": "vs-dark",
        "path": "./themes/Atomize-color-theme.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#282C34",
    "theme": "dark"
  },
  "scripts": {
    "fix": "prettier '**/*.{js,jsx,ts,tsx,json,md,yml}' --write",
    "lint": "prettier '**/*.{js,jsx,ts,tsx,json,md,yml}' --check",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md,yml}": "npm run fix"
  },
  "bugs": {
    "url": "https://github.com/anpleenko/atomize/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anpleenko/atomize.git"
  },
  "homepage": "https://github.com/anpleenko/atomize/blob/main/README.md",
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^16.1.6",
    "prettier": "^3.3.2"
  }
}
